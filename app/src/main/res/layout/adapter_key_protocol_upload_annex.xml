<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <ImageView
        android:id="@+id/adapter_key_protocol_upload_annex_iv"
        android:layout_width="0dp"
        android:layout_height="106dp"
        android:layout_marginTop="19dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/adapter_key_protocol_upload_annex_tv"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/adapter_key_protocol_upload_annex_delete_iv"
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:layout_constraintTop_toTopOf="@id/adapter_key_protocol_upload_annex_iv"
        app:layout_constraintRight_toRightOf="@id/adapter_key_protocol_upload_annex_iv"
        android:translationX="10dp"
        android:translationY="-10dp"
        android:background="@mipmap/proxy_delete_annex"/>



    <TextView
        android:id="@+id/adapter_key_protocol_upload_annex_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="附件类型:"
        android:textColor="#ff333333"
        android:textSize="14sp"
        android:layout_marginLeft="29dp"
        app:layout_constraintLeft_toRightOf="@id/adapter_key_protocol_upload_annex_iv"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/adapter_key_protocol_upload_annex_iv"/>

    <RelativeLayout
        android:id="@+id/type_1_rl"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toRightOf="@id/adapter_key_protocol_upload_annex_iv"
        android:layout_marginLeft="29dp"
        app:layout_constraintTop_toBottomOf="@id/adapter_key_protocol_upload_annex_tv"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="15dp">

        <TextView
            android:lines="1"
            android:ellipsize="end"
            android:layout_centerVertical="true"
            android:id="@+id/type_1_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15.5dp"
            android:text="请选择"
            android:textColor="#333333"
            android:textSize="13sp" />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>